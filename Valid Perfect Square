class Solution {
    public boolean isPerfectSquare(int num) {
       int start = 0;
       int high = num;
       
       while(start <= high){
        int mid = (start + high)/2;
        if(Math.pow(mid, 2) == num){
            return true;
        }else if(Math.pow(mid , 2) > num){
            high = mid -1;
        }else{
            start = mid +1 ;
        }
       } 
       return false;
    }
}
